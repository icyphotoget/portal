    "description": {
      "type": "richtext"
    },
    "items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::curated-list-item.curated-list-item",
      "mappedBy": "list"
    },
    "isPremium": {
      "type": "boolean"
    }
  }
}
